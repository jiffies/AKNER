# Unigram
U00:%x[-2,0]
U01:%x[-1,0]
U02:%x[0,0]
U03:%x[1,0]
U04:%x[2,0]
U05:%x[-1,0]/%x[0,0]
U06:%x[0,0]/%x[1,0]


U30:%x[-2,1]
U31:%x[-1,1]
U32:%x[0,1]
U33:%x[1,1]
U34:%x[2,1]
U35:%x[-2,1]/%x[-1,1]
U36:%x[-1,1]/%x[0,1]
U37:%x[0,1]/%x[1,1]
U38:%x[1,1]/%x[2,1]

U40:%x[-2,1]/%x[-1,1]/%x[0,1]
